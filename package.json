{
  "name": "code-red-calendar",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:api": "cd api && venv/bin/flask test",
    "start:prod": "NODE_ENV=production cd api && venv/bin/gunicorn --config gunicorn_config.py api:app",
    "start:dev": "NODE_ENV=development cd api && venv/bin/flask run --no-debugger",
    "build": "NODE_ENV=production webpack",
    "dev": "NODE_ENV=development webpack serve",
    "prod": "NODE_ENV=production nginx",
    "dev:all": "concurrently \"cross-env npm run start:dev\" \"cross-env npm run dev\"",
    "prod:all": "concurrently \"cross-env npm run start:prod\" \"cross-env npm run prod\"",
    "stop:prod": "nginx -s stop && kill -9 `lsof -i:5000 -t`",
    "webpack:analyze": "webpack --json > stats.json"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.6.2",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
